<h1>New Dog</h1>

<h2><%= flash[:notice] %> <%= @dog.errors.full_messages.join(', ') if @dog.errors.present? %></h2>

<%= form_for @dog do |f| %>
<%= f.label :name %>
<%= f.text_field :name %><br><br>

<%= f.label :breed %>
<%= f.text_field :breed %><br><br>

<%= f.label :age %>
<%= f.text_field :age %><br><br>

<%= f.submit "Create Dog" %>
<% end %>
